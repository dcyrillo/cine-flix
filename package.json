{
  "name": "cineflix",
  "version": "0.0.1",
  "description": "Movie manager [Nest](https://github.com/nestjs/nest) framework TypeScript starter repository.",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "10.x"
  },
  "main": "dist/src/main.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "client": "npm start --prefix client",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node main.js",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/src/main.js\"",
    "start:prod": "node main.js",
    "prestart:prod": "rimraf dist && npm run build",
    "postinstall": "npm run prestart:prod",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "test.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.1.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "dotenv-flow": "^3.1.0",
    "fastify-swagger": "^2.4.0",
    "mysql": "^2.17.1",
    "nest-router": "^1.0.9",
    "nestjs-config": "^1.4.4",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.1.1",
    "typeorm": "^0.2.18",
    "webpack-dev-server": "^3.8.1"
  },
  "devDependencies": {
    "@nestjs/testing": "6.1.1",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "prettier"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://dcyrillo@bitbucket.org/dcyrillo/desafio-haizen-cine-back.git"
  },
  "homepage": "https://bitbucket.org/dcyrillo/desafio-haizen-cine-back#readme"
}
